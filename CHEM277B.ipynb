    "import numpy as np \n",
    "\n",
    "def area(radius):\n",
    "    return np.pi*radius*radius \n",
    "\n",
    "def summation(num):\n",
    "    add = 0 \n",
    "    for i in range(num):\n",
    "        add += i \n",
    "    add += num\n",
    "    return add \n",
    "\n",
    "\n",
    "radius = 2 \n",
    "print(area(radius))"
   ]
